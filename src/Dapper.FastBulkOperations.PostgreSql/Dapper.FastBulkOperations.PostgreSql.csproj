<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <LangVersion>10</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>Extra Fast bulk Insert / Update / InserOrUpdate / Delete operations for Dapper</Description>
        <RepositoryUrl>https://github.com/filipppka/Dapper.FastBulkOperations</RepositoryUrl>
        <PackageTags>dapper, npgsql, postgresql, dapper bulk,bulkcopy,bulkmerge,insertorupdate,bulkdelete,bulkmerge,bulkinsert,dapper insert,dapper update,dapper delete,dapper merge</PackageTags>
        <PackageVersion>1.2.0</PackageVersion>
        <Title>Dapper.FastBulkOperations.PostgreSql</Title>
        <PackageProjectUrl>https://github.com/filipppka/Dapper.FastBulkOperations</PackageProjectUrl>
        <PackageLicenseUrl>https://licenses.nuget.org/Apache-2.0</PackageLicenseUrl>
        <license>https://licenses.nuget.org/Apache-2.0</license>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <Authors>Filipp Kleymenov</Authors>
        <PackageId>Dapper.FastBulkOperations.PostgreSql</PackageId>
        <TargetFramework>net8.0</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Dapper" Version="2.1.35" />
      <PackageReference Include="FastMember" Version="1.5.0" />
      <PackageReference Include="Npgsql" Version="8.0.3" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\FastBulkOperations.Root\FastBulkOperations.Root.csproj" />
    </ItemGroup>

</Project>
